{
  "name": "ember-user-activity",
  "version": "7.1.0",
  "description": "Ember Addon for tracking user activity & idling",
  "keywords": [
    "ember-addon",
    "user activity",
    "idle",
    "idling"
  ],
  "repository": "https://github.com/elwayman02/ember-user-activity",
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "volta": {
    "node": "14.21.1",
    "yarn": "1.22.19"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "https://ember-user-activity.jhawk.co"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
